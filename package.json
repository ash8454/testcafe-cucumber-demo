{
  "name": "testcafe-cucumber",
  "version": "0.1.0",
  "description": "An integration of TestCafe and CucumberJS",
  "author": "AshokT",
  "contributors": [
    "Ashok T"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rquellh/testcafe-cucumber"
  },
  "scripts": {
    "debug": "node --inspect=1337 --debug-brk --nolazy node_modules/cucumber/bin/cucumber-js --tags @debug --format json:./reports/report.json",
    "test": "NODE_ENV=local ./node_modules/.bin/cucumber-js",
    "test-report": "./node_modules/.bin/cucumber-js --format json:./reports/report.json",
    "test-chrome": "./node_modules/.bin/cucumber-js  --world-parameters \"{\\\"browser\\\": \\\"chrome\\\"}\""
  },
  "dependencies": {
    "base64-img": "^1.0.4",
    "cucumber": "^5.1.0",
    "eslint": "^4.19.1",
    "npm": "^6.0.0",
    "testcafe": "^1.1.0"
  },
  "devDependencies": {
    "cross-env": "^6.0.3",
    "custom-env": "^1.0.2",
    "eslint-config-google": "^0.9.1",
    "testcafe-react-selectors": "^3.3.0",
    "yargs": "^14.2.0"
  }
}
